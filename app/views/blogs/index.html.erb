<h1 class="jumbotron"> BLogs </h1>
<div class="container">
  <div class="row">
    <%= link_to 'Add Blog', new_blog_path, class: "btn btn-primary" %>
    <div class="col-12 card mt-3">
    	<ul>
	      <% @blogs.each do |blog| %>
				  <li>
				    <%= link_to(blog.title, blog, target: "_blank") %>
				  </li>
				<% end %>
    	</ul>
    </div>
  </div>
</div>